<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.berry.oss.core.mapper.ObjectInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.berry.oss.core.entity.ObjectInfo">
        <id column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="bucket_id" property="bucketId"/>
        <result column="file_name" property="fileName"/>
        <result column="file_path" property="filePath"/>
        <result column="file_id" property="fileId"/>
        <result column="hash" property="hash"/>
        <result column="category" property="category"/>
        <result column="size" property="size"/>
        <result column="acl" property="acl"/>
        <result column="formatted_size" property="formattedSize"/>
        <result column="is_dir" property="isDir"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, bucketId, user_id, file_name, file_path, file_id, hash, category, size,acl, formatted_size, is_dir, create_time, update_time
    </sql>

</mapper>
