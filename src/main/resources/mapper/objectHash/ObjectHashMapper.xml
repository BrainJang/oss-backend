<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.berry.oss.core.mapper.ObjectHashMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.berry.oss.core.entity.ObjectHash">
        <id column="id" property="id" />
        <result column="hash" property="hash" />
        <result column="file_id" property="fileId" />
        <result column="size" property="size" />
        <result column="create_time" property="createTime" />
        <result column="reference_count" property="referenceCount" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, hash, file_id, size, create_time, reference_count
    </sql>

</mapper>
